{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "api-route": {
        "ClusterId": "omg-api-cluster",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "omg-api-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "https+http://omg-api"
          }
        }
      }
    }
  }
}
